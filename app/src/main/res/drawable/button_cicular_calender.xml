<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape android:shape="oval" >
            <stroke
                android:height="1.0dip"
                android:width="1.0dip"
                android:color="#004381" />

            <solid android:color="#004F81" />

            <corners
                android:bottomLeftRadius="12.0dip"
                android:bottomRightRadius="12.0dip"
                android:radius="12.0dip"
                android:topLeftRadius="12.0dip"
                android:topRightRadius="12.0dip" />
        </shape>
    </item>
    <item>
        <shape android:shape="oval" >
            <gradient
                android:startColor="#7fd9d0"
                android:endColor="#62cec3"
                android:angle="270" />

            <corners
                android:bottomLeftRadius="12.0dip"
                android:bottomRightRadius="12.0dip"
                android:radius="12.0dip"
                android:topLeftRadius="12.0dip"
                android:topRightRadius="12.0dip" />
        </shape>
    </item>
</selector>